<div class="container">
  <h1>Add a new dose</h1>
  <div class="row">
    <div class="col-sm-4 ">
<%= simple_form_for [@cocktail, @dose] do |f| %>
  <%= f.input :description %>
  <%= f.association :ingredient %>
  <%= f.submit "add a dose", class: "btn btn-primary" %></div>
    <% end %>
  </div>
</div>
